{
  "name": "frontier-guest-checkin",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "jest && npm run test:e2e:headless",
    "test:unit": "jest --testPathPatterns=unit",
    "test:integration": "jest --testPathPatterns=integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**/*.{ts,tsx}' --coverageDirectory=coverage",
    "coverage:report": "node scripts/coverage-report.js",
    "coverage:full": "npm run test:coverage && npm run coverage:report",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:headless": "playwright test --project=chromium",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:e2e:headless",
    "test:quick": "jest --testPathPatterns=unit --passWithNoTests",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "test:legacy:multi": "tsx test/test-runner.ts",
    "test:legacy:scenarios": "tsx test/test-runner.ts scenarios",
    "test:legacy:generate": "tsx test/test-runner.ts generate",
    "test:legacy:all": "tsx test/test-runner.ts all"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-email/button": "^0.2.0",
    "@react-email/column": "^0.0.13",
    "@react-email/components": "^0.5.1",
    "@react-email/container": "^0.0.15",
    "@react-email/heading": "^0.0.15",
    "@react-email/html": "^0.0.11",
    "@react-email/render": "^1.2.1",
    "@react-email/row": "^0.0.12",
    "@react-email/section": "^0.0.16",
    "@react-email/text": "^0.1.5",
    "@types/qrcode": "^1.5.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jose": "^6.1.0",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "prisma": "^6.15.0",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-signature-canvas": "^1.1.0-alpha.2",
    "resend": "^6.0.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^10.0.0",
    "@playwright/test": "^1.44.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-signature-canvas": "^1.0.7",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jest-html-reporter": "^4.3.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5"
  }
}
